package com.revature;

import java.util.ArrayList;

public class Q1 {
	
	
	Integer[] numberList = new Integer[10];
	
	numnberList[1] = 1;
	numnberList[1] = 0;
    numnberList[1] = 5;	
    numnberList[1] = 6;	
    numnberList[1] = 3;	
    numnberList[1] = 2;	
    numnberList[1] = 3;	
    numnberList[1] = 7;	
    numnberList[1] = 9;	
    numnberList[1] = 8;	
    numnberList[1] = 4;	
    
	

	Integer[] public sort(Integer[] numberList) {
		for (int i=0; numberList.length; ++i) {
			for (int j = i+1; numberList.length; ++j) {
				if (numberList[i] > numberList[j+1]) {
					int currentElement = numberList[i];
					numberList[i] = numberList[j+1];
					numberList[j+1] = currentElement;
				} 
			}
		}
	}
	
	void public printNumberList(Integer[] numberList) {
		for (int i=0; numberList.length; ++i) {
			System.out.println(numberList[i]);
		}
	}

}
